# Makefile for src/compat/

SHELL = @SHELL@
top_srcdir = @top_srcdir@
srcdir = @srcdir@
VPATH = @srcdir@
depcomp = /bin/sh $(top_srcdir)/autotools/depcomp

@SET_MAKE@

CC = @CC@
LD = @CC@
STRIP = @STRIP@
CFLAGS = @CFLAGS@ -I../.. -I$(top_srcdir) -I$(top_srcdir)/src @DEFS@ $(CFLGS)
CPPFLAGS = @CPPFLAGS@

OBJS = inet_aton.o \
	inet_ntop.o \
	snprintf.o \
	memset.o \
	memcpy.o \
	strcasecmp.o \
	strftime.o

doofus:
	@echo ""
	@echo "Let's try this from the right directory..."
	@echo ""
	@cd ../.. && $(MAKE)

clean:
	@rm -f .depend *.o *~

compat: $(OBJS)

include ./.deps/includes

.SUFFIXES:
.SUFFIXES: .c .o .h

.c.o:
	@echo -e "Compiling: \033[1m`basename $< .c`\033[0m"
	@source='$<' object='$@' depfile='.deps/$*.Po' tmpdepfile='.deps/$*.TPo' depmode=$(CCDEPMODE) $(depcomp) \
	$(CC) $(CFLAGS) -I../.. -I$(top_srcdir) -I$(top_srcdir)/src $(CPPFLAGS) -c $< 


#safety hash
